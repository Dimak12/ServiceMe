<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HouseCleaning</servlet-name>
        <servlet-class>ServiceMe.CalculateTotal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogIn</servlet-name>
        <servlet-class>ServiceMe.LogIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cust_SignUp</servlet-name>
        <servlet-class>ServiceMe.Sign</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sign</servlet-name>
        <servlet-class>ServiceMe.Sign</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpCust</servlet-name>
        <servlet-class>ServiceMe.SignUpCust</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Session</servlet-name>
        <servlet-class>ServiceMe.Session</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Try</servlet-name>
        <servlet-class>ServiceMe.Try</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>ServiceMe.LogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TotalCalc</servlet-name>
        <servlet-class>ServiceMe.TotalCalc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HouseCleaning</servlet-name>
        <url-pattern>/HouseCleaning</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogIn</servlet-name>
        <url-pattern>/LogIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cust_SignUp</servlet-name>
        <url-pattern>/Cust_SignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sign</servlet-name>
        <url-pattern>/Sign</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpCust</servlet-name>
        <url-pattern>/SignUpCust</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Session</servlet-name>
        <url-pattern>/Session</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Try</servlet-name>
        <url-pattern>/Try</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TotalCalc</servlet-name>
        <url-pattern>/TotalCalc</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
